require "nokogiri"
include Nokogiri
f = FileList["../model/**/*.xml"]
elem = f.flat_map do |f|
  xml = XML.parse File.read(f)
  xml.at_css("SteerBenchTestCase").elements
end;
elemgrp = elem.group_by(&:name);
obstacle = elemgrp["obstacle"]
getcname = Proc.new do |c|
  c.elements.map(&:name)
end
obstacle.flat_map(&getcname).uniq
$: << Dir.pwd
requrie "CrowdComm/main"
